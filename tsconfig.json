{
	"compilerOptions": {
		"target": "esnext",
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"noEmit": true,
		"strict": false,
		"rootDir": ".",
		"skipLibCheck": true,
		"noImplicitThis": true,
		"rewriteRelativeImportExtensions": true,
		"allowImportingTsExtensions": true,
		"forceConsistentCasingInFileNames": true
	}
}
