{
	"compilerOptions": {
		"target": "ESNext",
		"module": "preserve",
		"allowJs": true,
		"checkJs": false,
		"rootDir": ".",
		"strict": true,
		"skipLibCheck": true,
		"noImplicitThis": true,
		"noEmit": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["lib/**/**/*", "src/cache.ts", "src/auth.ts", "src/group.ts"]
}
